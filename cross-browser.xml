<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Feature Suite" parallel="tests"
	preserve-order="true" thread-count="2" verbose="5">

	<test name="Firefox Browser Test" parallel="methods" thread-count="5">
		<parameter name="browser" value="Firefox" />
		<classes>
			<class name="com.dgsl.selenium.scenario.CrossBrowser" />
		</classes>
	</test>

	<test name="Chrome Browser Test" parallel="methods" thread-count="5">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.dgsl.selenium.scenario.CrossBrowser" />
		</classes>
	</test>

</suite>
